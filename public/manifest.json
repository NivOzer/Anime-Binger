{
    "version": "1.0.0",
    "manifest_version": 3,
    "name": "React Chrome Extension",
    "description": "This is a Chrome extension built with React and TypeScript",
    "action": {
        "default_popup": "index.html",
        "default_title": "Anime Binger"
    },
    "icons": {
        "16": "/icons/headIcon16.png",
        "32": "/icons/headIcon32.png",
        "48": "/icons/headIcon48.png",
        "128": "/icons/headIcon128.png"
      },
      "permissions": [
        "cookies",
        "storage"
      ]
}